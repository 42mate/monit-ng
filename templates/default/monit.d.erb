# <%= @name %> check generated by Chef for <%= node.fqdn %>
check <%= @service_type %> <%= @name %> with <%= @id_type %> <%= @service_id %>
<% if @service_group -%>
  group <%= @service_group %>
<% end -%>
<% if @start_command -%>
  start "<%= @start_command %>"
<% end -%>
<% if @stop_command -%>
  stop "<%= @stop_command %>"
<% end -%>
<% @service_tests.each do |service_test| -%>
  <%= service_test['condition'] %>
    then <%= service_test['action'] %>
<% end -%>
