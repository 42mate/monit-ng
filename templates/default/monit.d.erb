# <%= @name %> check generated by Chef
check <%= @service_type %> <%= @name %> 
<% if @id_type %>
  with <%= @id_type %> <%= @service_id %>
<% end %>
<% if @every %>
  every <%= @every %>
<% end %>
<% if @service_group %>
  group <%= @service_group %>
<% end %>
<% if @start_command %>
  start "<%= @start_command %>"
<% end %>
<% if @start_as %>
    as uid <%= @start_as %>
<% end %>
<% if @stop_command %>
  stop "<%= @stop_command %>"
<% end %>
<% @service_tests.each do |service_test| %>
  if <%= service_test['condition'] %>
    then <%= service_test['action'] %>
<% end %>
